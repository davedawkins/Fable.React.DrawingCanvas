{
  "name": "fable-react-drawingcanvas",
  "private": false,
  "version": "1.2.0",
  "repository": {
    "url": "https://github.com/davedawkins/Fable.React.DrawingCanvas.git"
  },
  "scripts": {
    "build": "webpack -p",
    "start:clock": "webpack-dev-server --config webpack.clock.config.js",
    "build:clock": "webpacK --config webpack.clock.config.js",
    "start:bubbles": "webpack-dev-server --config webpack.bubbles.config.js",
    "build:bubbles": "webpacK --config webpack.bubbles.config.js",
    "start:fractal": "webpack-dev-server --config webpack.fractal.config.js",
    "build:fractal": "webpacK --config webpack.fractal.config.js",
    "start:particles": "webpack-dev-server --config webpack.particles.config.js",
    "build:particles": "webpacK --config webpack.particles.config.js",
    "start:turtle": "webpack-dev-server --config webpack.turtle.config.js",
    "build:turtle": "webpacK --config webpack.turtle.config.js",
    "publish": "node publish.js",
    "pretest": "fable-splitter tests -o dist/tests --commonjs",
    "test": "mocha dist/tests"  },
  "dependencies": {
    "bulma": "^0.9.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
