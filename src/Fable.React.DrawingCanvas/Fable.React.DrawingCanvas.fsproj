<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <Description>Fable.React binding for HTML5 canvas</Description>
    <PackageProjectUrl>https://github.com/davedawkins/Fable.React.DrawingCanvas</PackageProjectUrl>
    <RepositoryUrl>https://github.com/davedawkins/Fable.React.DrawingCanvas</RepositoryUrl>
    <PackageLicenseUrl>https://github.com/davedawkins/Fable.React.DrawingCanvas/blob/master/LICENSE</PackageLicenseUrl>
    <PackageIconUrl></PackageIconUrl>
    <PackageTags>fsharp;fable</PackageTags>
    <Authors>Dave Dawkins</Authors>
    <Version>1.3.1</Version>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
  </PropertyGroup>
  <ItemGroup>
    <Compile Include="DrawingCanvas.fs" />
  </ItemGroup>
  <ItemGroup>
    <Content Include="*.fsproj; *.fs;" PackagePath="fable\" />
  </ItemGroup>
  <Import Project="..\..\.paket\Paket.Restore.targets" />
</Project>